<mapper namespace="com.example.demo.mapper.LeaveapplicationMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.pojo.Leaveapplication">
            <result property="leaveId" column="leave_id" />
            <result property="employeeId" column="employee_id" />
            <result property="startDate" column="start_date" />
